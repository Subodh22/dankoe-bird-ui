{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/index.js"
    }
  ],
  "crons": [
   
    {
      "path": "/api/cron/foryou?secret=@cron_secret",
      "schedule": "0 21 * * *"
    }
  ]
}
