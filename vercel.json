{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/index.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/fetch",
      "schedule": "0 20 * * *",
      "headers": {
        "x-cron-secret": "@cron_secret"
      }
    },
    {
      "path": "/api/cron/foryou",
      "schedule": "0 21 * * *",
      "headers": {
        "x-cron-secret": "@cron_secret"
      }
    }
  ]
}
